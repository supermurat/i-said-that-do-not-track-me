{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/supermurat/i-said-that-do-not-track-me",
  "author": "Murat Demir",
  "default_locale": "en",
  "version": "1.2.6",
  "applications": {
    "gecko": {
      "id": "i-said-that-do-not-track-me@supermurat.com",
      "strict_min_version": "57.0"
    }
  },
  "icons": {
    "48": "icon.svg"
  },
  "browser_action": {
    "default_icon": {
      "48": "icon.svg"
    },
    "default_title": "__MSG_actionTitle__",
    "default_popup": "views/popup.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "options_ui": {
    "page": "views/options.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
